{
    "dataset": {
        "name": "mnist",
        "download": false,
        "batch_size": 64,
        "num_workers": 2
    },
    "device": "cuda",
    "model": {
        "input_dim": 1,
        "output_dim": 10
    },
    "train": {
        "optim": {
            "name": "SGD",
            "params": {
                "lr": 1e-2
            }
        },
        "num_epochs": 128,
        "print_every": 200
    }
}